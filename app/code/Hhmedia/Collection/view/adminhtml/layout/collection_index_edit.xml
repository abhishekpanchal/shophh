<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="jquery/fileUploader/css/jquery.fileupload-ui.css"/>
        <link src="jquery/fileUploader/bootstrap.js"/>
    </head>
    <update handle="editor"/>
    <body>
        <referenceContainer name="content">
            <block class="Hhmedia\Collection\Block\Adminhtml\Collection\Edit" name="collection_edit"/>
        </referenceContainer>
        <referenceContainer name="left">
            <block class="Hhmedia\Collection\Block\Adminhtml\Collection\Edit\Tabs" name="collection_edit_tabs">
                <block class="Hhmedia\Collection\Block\Adminhtml\Collection\Edit\Tab\Main" name="collection_edit_tab_main"/>
                <block class="Hhmedia\Collection\Block\Adminhtml\Collection\Edit\Tab\Image" name="collection_edit_tab_image"/>
