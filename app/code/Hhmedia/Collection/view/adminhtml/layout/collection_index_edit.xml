                <block class="Hhmedia\Collection\Block\Adminhtml\Collection\Edit\Tab\SectionEight" name="collection_edit_tab_section_eight"/>
                <action method="addTab">
                    <argument name="name" xsi:type="string">main_section</argument>
                    <argument name="block" xsi:type="string">collection_edit_tab_main</argument>
                </action>
                <action method="addTab">
                    <argument name="name" xsi:type="string">image_section</argument>
                    <argument name="block" xsi:type="string">collection_edit_tab_image</argument>
                </action>
                <action method="addTab">
                    <argument name="name" xsi:type="string">section_one</argument>
                    <argument name="block" xsi:type="string">collection_edit_tab_section_one</argument>
                </action>
                <action method="addTab">
                    <argument name="name" xsi:type="string">section_two</argument>
                    <argument name="block" xsi:type="string">collection_edit_tab_section_two</argument>
